{
  "name": "sample-app",
  "description": "",
  "author": "",
  "version": "0.1.0",
  "homepage": "",
  "license": "",
  "main": "app.js",
  "readmeFilename": "",
  "engines": {
    "node": ">=0.10.0"
  },
  "engineStrict": true,
  "dependencies": {
    "roosevelt": "^0.5.0",
    "teddy": "^0.3.0",
    "roosevelt-less": "^0.2.0",
    "roosevelt-closure": "^0.1.0"
  },
  "devDependencies": {
    "jshint": "*",
    "nodemon": "*"
  },
  "rooseveltConfig": {
    "staticsRoot": "statics",
    "cssPath": "css",
    "cssCompiler": {
      "nodeModule": "roosevelt-less",
      "params": {
        "compress": true
      }
    },
    "cssCompilerWhitelist": null,
    "cssCompiledOutput": ".build/css",
    "jsCompiler": {
      "nodeModule": "roosevelt-closure",
      "params": {
        "compilation_level": "ADVANCED_OPTIMIZATIONS"
      }
    }
  },
  "jshintConfig": {
    "camelcase": true,
    "curly": true,
    "devel": true,
    "indent": 2,
    "node": true
  },
  "private": true,
  "repository": {
    "private-repo": "git+ssh://somewhere:port/folder/on/server"
  },
  "scripts": {
    "start": "nodemon -e js,json,less,html app.js --ignore node_modules/",
    "test": "jshint app.js"
  }
}
