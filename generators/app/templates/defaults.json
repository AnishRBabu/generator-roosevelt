{
  "dependencies": {
    "roosevelt": "~0.15.1"
  },
  "appName": "My Roosevelt Sample App",
  "createDir": true,
  "httpPort": 43711,
  "https": false,
  "stateName": "n/a",
  "localityName": "n/a",
  "organizationName": "n/a",
  "organizationalUnit": "n/a",
  "altURI": "http://localhost/",
  "altIP": "127.0.0.1",
  "defaultCSSCompiler": "rooseveltLess",
  "rooseveltLess": {
    "dependencies": {
      "roosevelt-less": "~0.8.0"
    },
    "options": {
      "nodeModule": "roosevelt-less",
      "params": {
        "sourceMap": null
      }
    },
    "scripts": {
      "cssExt": "less",
      "cssSyntax": " --syntax less"
    }
  },
  "rooseveltSass": {
    "dependencies": {
      "roosevelt-sass": "~0.4.1"
    },
    "options": {
      "nodeModule": "roosevelt-sass",
      "params": {
        "outputStyle": "compressed"
      }
    },
    "scripts": {
      "cssExt": "scss",
      "cssSyntax": " --syntax scss"
    }
  },
  "defaultJSCompiler": "rooseveltUglify",
  "rooseveltUglify": {
    "dependencies": {
      "roosevelt-uglify": "~0.3.2"
    },
    "options": {
      "nodeModule": "roosevelt-uglify",
      "showWarnings": false,
      "params": {}
    }
  },
  "rooseveltClosure": {
    "dependencies": {
      "roosevelt-closure": "~0.5.3"
    },
    "options": {
      "nodeModule": "roosevelt-closure",
      "showWarnings": false,
      "params": {
        "compilationLevel": "ADVANCED"
      }
    }
  },
  "modelsPath": "mvc/models",
  "viewsPath": "mvc/views",
  "controllersPath": "mvc/controllers",
  "templatingEngine": true,
  "templatingEngineName": "teddy",
  "templatingExtension": "html",
  "additionalTemplatingEngines": false,
  "teddy": {
    "teddy": "~0.4.28"
  },
  "viewEngine": [
    "html:teddy"
  ]
}
